{
  "name": "eufony/cache-tests",
  "description": "Unit tests for PSR-6 and PSR-16 cache implementations.",
  "keywords": [ "eufony", "cache", "psr-6", "psr-16", "testsuite" ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alpin Gencer",
      "email": "alpingencer@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.0",
    "phpunit/phpunit": "^9.5",
    "psr/cache": "^1.0 || ^2.0 || ^3.0",
    "psr/simple-cache": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Eufony\\Cache\\Tests\\": [ "src/" ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
